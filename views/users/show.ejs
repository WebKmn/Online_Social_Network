<div class="container margin-from-top margin-from-bottom">
    <h2 class="center">User Data for <%= user.fullName %>
    </h2>
    <table class="table">
        <tbody>
            <tr>
                <td>Name</td>
                <td>
                    <%= user.fullName %>
                </td>
            </tr>
            <tr>
                <td>Username</td>
                <td>
                    <%= user.username %>
                </td>
            </tr>
            <tr>
                <td>Email</td>
                <td>
                    <%= user.email %>
                </td>
            </tr>
            <tr>
                <td>Location</td>
                <td>
                    <%= user.location %>
                </td>
            </tr>
            <tr>
                <td>Gender</td>
                <td>
                    <%= user.gender %>
                </td>
            </tr>
            <tr>
                <td>Date of Birth</td>
                <td>
                    <%= user.DOB %>
                </td>
            </tr>
            <tr>
                <td>Biography</td>
                <td>
                    <%= user.bio %>
                </td>
            </tr>
        </tbody>
    </table>
</div>